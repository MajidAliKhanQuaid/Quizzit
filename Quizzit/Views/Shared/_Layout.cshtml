<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")
    @*<script src="~/Scripts/venta/three.r92.min.js"></script>
    <script src="~/Scripts/venta/vanta.net.min.js"></script>*@
</head>
<body>
    <div class="container">
        <div class="col-md-4">
            <img id="logo" class="flash animated" src="~/Content/images/logo.png" />
        </div>
        <div class="col-md-8"></div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Quizzit</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    @*<script>
            VANTA.NET({
                el: "body",
                color: 0x0,
                backgroundColor: 0xffffff
            });

        </script>*@
</body>
</html>
